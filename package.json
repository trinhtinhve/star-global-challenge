{
  "name": "star-global-challenge",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./src/app.js",
    "start_watch": "pm2 start ./src/app.js --watch --no-daemon -f",
    "stop_all": "pm2 stop all",
    "start_cluster": "pm2 start ./src/app.js -i max",
    "create_db": "node ./create_db.js",
    "migration.gen": "npx sequelize-cli migration:generate --name",
    "migration.migrate": "npx sequelize-cli db:migrate",
    "test": "NODE_ENV=test jest --runInBand"
  },
  "dependencies": {
    "bluebird": "^3.7.2",
    "config": "^3.3.3",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "morgan": "~1.9.1",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "pm2": "^4.5.0",
    "sequelize": "^6.3.5",
    "underscore": "^1.12.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "jest": "^26.6.3",
    "pg-connection-string": "^2.4.0",
    "pgtools": "^0.3.0",
    "sequelize-cli": "^6.2.0",
    "supertest": "^6.0.1"
  }
}
